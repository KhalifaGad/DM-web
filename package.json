    "ejs": "2.6.1",
    "eslint": "5.16.0",
    "firebase-admin": "^8.7.0",
    "graphql": "14.3.1",
    "graphql-cli": "^3.0.12",
    "graphql-type-json": "^0.3.0",
    "jsonwebtoken": "8.5.1",
    "nodemailer": "6.4.16",
    "nodemon": "1.19.1",
